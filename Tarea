##Tarea karla Hernández
bd <- c(3.4,4.8,3.3,5.6,3.2,2.9,1.9,2.8,6.0,4.3,2.2)
st <- ts(bd,start = c(2005,1),freq=4)
x <- data.frame(st)
jpeg("GraficaR.jpg")
layout(1:2)
plot(x)
plot(st)
plot(x,xlab = "Años",ylab = "Tasa de Desocupación",main="Tasa de Desocupación")
plot(aggregate(st),xlab = "Años",ylab = "Tasa de Desocupación",main="Tendencia")
dev.off()
